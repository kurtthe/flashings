{
  "name": "flashings",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "reset": "npx rimraf **/node_modules && yarn install",
    "pod:install": "cd ios && bundle exec pod install --clean-install",
    "pod:clean": "cd ios && pod cache clean --all&& rm -rf Podfile.lock Pods",
    "ios:dev": "concurrently --raw 'react-native run-ios --no-packager --simulator=\"iPhone 14\"' 'yarn start'",
    "android:dev": "yarn jetify && concurrently --raw \"react-native run-android --variant=devDebug --appIdSuffix=dev --no-packager --no-jetifier\" \"yarn start\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@shopify/restyle": "^2.4.2",
    "@tanstack/react-query": "^4.29.19",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.1",
    "color": "^4.2.3",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "formik": "^2.4.2",
    "lodash": "^4.17.21",
    "metro-config": "^0.77.0",
    "react": "18.2.0",
    "react-native": "0.72.1",
    "react-native-animatable": "^1.3.3",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "^4.6.4",
    "react-native-screens": "^3.22.1",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "@tsconfig/react-native": "^3.0.0",
    "@types/color": "^3.0.3",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-shape": "^3.1.1",
    "@types/lodash": "^4.14.195",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^3.0.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "rimraf": "^5.0.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
